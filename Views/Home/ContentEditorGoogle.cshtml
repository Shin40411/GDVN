@if (ViewBag.LinkExist != null && ViewBag.LinkExist == true)
{
    <div class="col-lg-12 col-md-12 iframe-content">
        <iframe src="@Html.Raw(ViewBag.EditorLink)" frameborder="0" allowfullscreen="true" mozallowfullscreen="true" webkitallowfullscreen="true"></iframe>"
    </div>
}
else
{
    <div class="col-lg-12">
        <div class="alert-box primary-alert">
            <div class="alert">
                <h4 class="alert-heading">THÔNG BÁO</h4>
                <p class="text-medium">
                    Chưa có dữ liệu giáo án.
                </p>
            </div>
        </div>
    </div>
}