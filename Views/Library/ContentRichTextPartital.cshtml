@if (ViewBag.DocumentUrl != null)
{
    Html.DevExpress().RichEdit(settings =>
    {
        settings.Name = "richEditContent";
        settings.RibbonMode = DevExpress.Web.ASPxRichEdit.RichEditRibbonMode.None;
        settings.ReadOnly = true;
        settings.Width = Unit.Percentage(100);

        settings.Settings.Views.ViewType = DevExpress.Web.ASPxRichEdit.RichEditViewType.Simple;

    }).Open(Server.MapPath(ViewBag.DocumentUrl)).GetHtml();
}
else
{
    <div class="col-lg-12">
        <div class="alert-box primary-alert">
            <div class="alert">
                <h4 class="alert-heading">THÔNG BÁO</h4>
                <p class="text-medium">
                    Chưa tồn tại tập tin này.
                </p>
            </div>
        </div>
    </div>
}

