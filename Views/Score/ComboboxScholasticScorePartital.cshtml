@Html.DevExpress().ComboBox(settings =>
{
    settings.Name = "comboBoxScholasticScoreFilter";
    settings.Width = Unit.Percentage(100);
    settings.Properties.NullText = "Chọn năm học";
    settings.Properties.TextField = "Name";
    settings.Properties.ValueField = "ID";
    settings.Properties.ValueType = typeof(int);
    settings.Properties.ClientSideEvents.SelectedIndexChanged = "ScholasticScoreFilterSelectedIndexChanged";
}).BindList(ViewBag.ScholasticScoreFilterData).Bind(ViewBag.ScholasticScoreFilterDataID).GetHtml()